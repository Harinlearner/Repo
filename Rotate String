class Solution {
public:
    bool rotateString(string s, string goal) {
        if(s.length()!=goal.length())
        {
            return false;
        }
        string str=s;
        for(int i=0;i<str.length();i++)
        {
            s+=s[i];
            if(goal==s.substr(i+1))
            {
                return true;
            }
        }
        return false;
    }
};
