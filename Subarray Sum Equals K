class Solution {
public:
    int subarraySum(vector<int>&a, int k) {
        map<int,int>mp;
        int count=0;
        int sum=0;
        int n=a.size();
        mp[0]=1;
        for(int i=0;i<n;i++)
        {
            sum+=a[i];
            int f=sum-k;
            if(mp[f]!=0)
            {
                count+=mp[f];
            }
            mp[sum]++;
            
        }
       
        return count;
    }
};
