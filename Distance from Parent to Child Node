int Dist(Node *root,int a,int lvl)
    {
       if(root==NULL)
       {
           return -1;
       }
       if(root->data==a)
       {
           return lvl;
       }
       int left=minDist(root->left,a,lvl+1);
       if(left!=-1)
       {
           return left;
       }
       return minDist(root->right,a,lvl+1);
    }
