
    TreeNode* find(int i,int j,vector<int>&nums)
    {
        if(j<i)
        {
            return NULL;
        }
        TreeNode *newnode=new TreeNode();
        int mid=(i+j)/2;
        newnode->val=nums[mid];
        newnode->left=find(i,mid-1,nums);
        newnode->right=find(mid+1,j,nums);
        return newnode;
    }
    TreeNode* sortedArrayToBST(vector<int>& nums) {
    // Node *root==NULL;
    int i=0;
    int j=nums.size()-1;
    return find(i,j,nums);
    }
